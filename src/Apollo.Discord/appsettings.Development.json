{
  "GrpcHostConfig": {
    "Host": "apollo-service",
    "Port": 5270,
    "ApiToken": "this_is_a_sample_token_from_env",
    "ValidateSslCertificate": false,
    "UseHttps": false
  },
  "ConnectionStrings": {
    "Redis": "localhost:6379,password=apollo_redis"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Discord": {
    "Token": "your_discord_bot_token_here",
    "PublicKey": "your_discord_public_key_here"
  }
}